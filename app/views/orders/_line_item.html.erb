<tr>
  <td>
    <%= image_tag line_item.product.image.tiny.url %>
  </td>
  <td>
    <h4><%= line_item.product.name %></h4>
    <br>
    <p><%= line_item.product.description %></p>
  </td>
  <td>
    <%= humanized_money_with_symbol line_item.item_price_cents/100 %>
  </td>
  <td>
    <div>
    <%= line_item.quantity %>
   
  </td>
  <td>
    <%= humanized_money_with_symbol line_item.total_price_cents/100 %>
  </td>
</tr>
